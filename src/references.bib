  issn = {1358-2461, 1755-3555},
  doi = {10.1017/S1358246100002435},
  abstract = {In recent years there has been a great deal of discussion about the prospects of developing a `naturalized epistemology', though different authors tend to interpret this label in quite different ways. One goal of this paper is to sketch three projects that might lay claim to the `naturalized epistemology' label, and to argue that they are not all equally attractive. Indeed, I'll maintain that the first of the three\textemdash the one I'll attribute to Quine\textemdash is simply incoherent. There is no way we could get what we want from an epistemological theory by pursuing the project Quine proposes. The second project on my list is a naturalized version of reliabilism. This project is not fatally flawed in the way that Quine's is. However, it's my contention that the sort of theory this project would yield is much less interesting than might at first be thought.},
  file = {/home/vassilis/Sync/Zotero/Stich_1993_Naturalizing Epistemology.pdf},
  journal = {Royal Institute of Philosophy Supplement},
  language = {en}
}

